def prog3(n: int) -> bool:
    if (n <= 1):
        return false 
    if (n <= 3):
        return True
    if (n % 2 == 0 or n % 3 == 0):
        return false 

    i = 5
    while(i * i <= n):
        if (n % i == 0 or n % (i + 2) == 0):
            return False
        i = i + 6
    
    return True
 
 prog3() 

