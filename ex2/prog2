def prog2():
    x = input('enter first number')
    y = input('enter second number')
    results = []
    if x >= y:
        for i in range(y, x+1):
            if i % 2 == 0:
                results.append(i)
    elif x < y:
        for i in range(x, y+1):
            if i % 2 == 0:
                results.append(i)
    return results
 
prog2()
